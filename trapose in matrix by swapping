
#include <iostream>
#include<vector>
using namespace std;
int main() {
int m,n;
cout<<"enter the number of rows";
cin>>m;

int j;
int arr[m][m];
cout<<"enter the values in array1";
for(int i=0;i<m;i++)
{
    for(int j=0;j<m;j++)
    {
        cin>>arr[i][j];
    }
}
//
for(int i=0;i<m;i++)
{
    for(int j=0;j<m;j++)
    {
        cout<<arr[i][j]<<" ";
    }
    cout<<endl;
}

for(int i=0;i<m;i++)
{
    for(int j=i+1;j<m;j++)
    {
        int temp=arr[i][j];
        arr[i][j]=arr[j][i];
        arr[j][i]=temp;
    }
    cout<<endl;
}
//
for(int i=0;i<m;i++)
{
    for(int j=0;j<m;j++)
    {
        cout<<arr[i][j]<<" ";
    }
    cout<<endl;
}



    return 0;
}
