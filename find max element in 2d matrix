
#include <iostream>
#include<vector>
using namespace std;
int main() {
#include <iostream>
#include<vector>
using namespace std;
int main() {
int m,n;
cout<<"enter the number of rows";
cin>>m;
cout<<"enter the number of column";
cin>>n;
int arr[m][n];
cout<<"enter the values in array";
for(int i=0;i<m;i++)
{
    for(int j=0;j<n;j++)
    {
        cin>>arr[i][j];
    }
}
for(int i=0;i<m;i++)
{
    for(int j=0;j<n;j++)
    {
        cout<<arr[i][j]<<" ";
    }
    cout<<endl;
}


    return 0;
}
int m,n;
cout<<"enter the number of rows";
cin>>m;
cout<<"enter the number of column";
cin>>n;
int arr[m][n];
cout<<"enter the values in array";
for(int i=0;i<m;i++)
{
    for(int j=0;j<n;j++)
    {
        cin>>arr[i][j];
    }
}
//max

int max=arr[0][0];
for(int i=0;i<m;i++)
{
    for( int j=0;j<n;j++)
    {
        if(max<arr[i][j])
        {
            max=arr[i][j];
        }
    }
}
cout<<max<<endl;
for(int i=0;i<m;i++)
{
    for(int j=0;j<n;j++)
    {
        cout<<arr[i][j]<<" ";
    }
    cout<<endl;
}


    return 0;
}
